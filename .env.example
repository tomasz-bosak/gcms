# This file is used by docker-compose and Flask to set environment variables.
# It should NOT be committed to version control.
# Copy this file to .env and fill in your actual secrets.

FLASK_APP=run.py
FLASK_ENV=development
SECRET_KEY=your_super_secret_key_here_change_me
# MariaDB settings
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=golf_db
MYSQL_USER=golf_user
MYSQL_PASSWORD=golf_password
SQLALCHEMY_DATABASE_URI=mysql+mysqlconnector://golf_user:golf_password@db/golf_db
